#!/usr/bin/env php
<?php

/**
 * if we're running from phar load the phar autoload,
 * else let the script 'robo' search for the autoloader
 */

foreach (array(__DIR__ . '/../../../autoload.php', __DIR__ . '/../../autoload.php', __DIR__ . '/../vendor/autoload.php', __DIR__ . '/vendor/autoload.php') as $file) {
    if (file_exists($file)) {
        require_once $file;
        break;
    }
}

$path = ComposerLocator::getPath("drupal/workbench_moderation");

print $path;


